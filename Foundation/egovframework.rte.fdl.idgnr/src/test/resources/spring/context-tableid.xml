<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

	<bean name="basicService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource" />
		<property name="blockSize" value="10" />
		<property name="table" value="idttest" />
		<property name="tableName" value="id" />
   </bean>

	<bean name="Ids-TestSimpleRequestIdsSize1" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
	</bean>

	<bean name="Ids-TestSimpleRequestIdsSize10" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
	</bean>

	<bean name="Ids-TestSimpleRequestIdsSize100" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" 	value="100"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
	</bean>

	<bean name="Ids-TestBigDecimalRequestIdsSize10" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="useBigDecimals" value="true"/>
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
	</bean>

	<bean name="Ids-TestMaxByteIds" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
	</bean>

	<bean name="Ids-TestMaxShortIds" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
	</bean>

	<bean name="Ids-TestMaxIntegerIds" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
	</bean>

	<bean name="Ids-TestMaxLongIds" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" 	value="10"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
	</bean>

	<bean name="Ids-TestNotDefinedTableInfo" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" value="1"/>
	</bean>

	<bean name="Ids-TestWithGenerationStrategy" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="strategy" ref="strategy"/>
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
	</bean>

	<bean name="strategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
		<property name="prefix" value="TEST-" />
		<property name="cipers" value="5" />
		<property name="fillChar" value="*" />
	</bean>

	<bean name="Ids-TestNonExistingTableName" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="does-not-exist"/>
	</bean>

	<bean name="Ids-TestWithColumnName" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
		<property name="dataSource" ref="dataSource"/>
		<property name="blockSize" 	value="1"/>
		<property name="table"	   	value="idttest"/>
		<property name="tableName"	value="test"/>
		<property name="tableNameFieldName" value="table_name"/>
		<property name="nextIdFieldName" value="next_id"/>
	</bean>
</beans>
